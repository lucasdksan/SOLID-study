// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Teacher {
  id       String   @id @default(uuid())
  name     String
  date     DateTime
  subjects String   @unique
}

model Student {
  id   String   @id @default(uuid())
  name String
  date DateTime
}

model Subject {
  id         String   @id @default(uuid())
  name       String
  createDate DateTime
  teacher    String
}
